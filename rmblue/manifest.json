{
	"manifest_version": 3,
	"name": "rmblue",
	"version": "1.0.1",
	"description": "Removes the Twitter Blue sign-up button from the sidebar in the Twitter Web App.",
	"icons":{
		"48": "icons/rmblue-48.png"
	},
	"content_scripts": [
		{
			"matches": [
				"*://*.twitter.com/*"
			],
			"js": ["rmblue.js"]
		}
	],
	"browser_specific_settings": {
		"gecko": {
			"id": "rmblue@rmblue"
		}
	}
}
